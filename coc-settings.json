{
  "coc.preferences.formatOnSave": true,
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.enableLinkedEditing": true,
  "coc.source.around.priority": 1,
  "coc.source.buffer.priority": 1,
  "html.autoClosingTags": true,
  "coc.source.file.priority": 99,
  "coc.source.word.priority": 1,
  "coc.source.word.filetypes": ["coc_todolist"],
  "diagnostic.enableSign": false,
  "diagnostic.floatConfig": {
    "border": true
  },
  "eslint.enable": true,
  "hover.floatConfig": {
    "border": true
  },
  "languageserver": {
    "lua": {
      "args": ["--stdio"],
      "command": "lua-language-server",
      "filetypes": ["lua"]
    },
    "emmet": {
      "args": ["--stdio"],
      "command": "emmet-language-server",
      "filetypes": ["css", "html", "javascriptreact", "javascript"]
    }
  },
  "prettier.enable": true,
  "signature.floatConfig": {
    "border": true
  },
  "snippets.textmateSnippetsRoots": ["~/.config/nvim/snippets"],
  "suggest.snippetsSupport": true,
  "snippets.priority": 99,
  "snippets.snipmate.enable": false,
  "snippets.ultisnips.enable": true,
  "suggest.completionItemKindLabels": {
    "class": " פּ ",
    "color": "  ",
    "constant": "  ",
    "constructor": "  ",
    "enum": "  ",
    "enumMember": "  ",
    "event": "  ",
    "field": "  ",
    "file": "  ",
    "folder": "  ",
    "function": "  ",
    "interface": " 蘒 ",
    "keyword": "  ",
    "method": "  ",
    "module": "  ",
    "operator": "  ",
    "property": "  ",
    "reference": "  ",
    "snippet": "  ",
    "struct": "  ",
    "text": "  ",
    "typeParameter": "  ",
    "unit": "  ",
    "value": "  ",
    "variable": "[]"
  },
  "suggest.defaultSortMethod": "alphabetical",
  "suggest.enablePreselect": true,
  "suggest.floatConfig": {
    "border": true
  },
  "suggest.formatItems": ["kind", "abbr", "menu", "shortcut"],
  "suggest.languageSourcePriority": 99,
  "suggest.maxCompleteItemCount": 28,
  "suggest.noselect": false,
  "suggest.removeCurrentWord": true,
  "suggest.removeDuplicateItems": true,
  "suggest.virtualText": true,
  "snippets.ultisnips.pythonPrompt": false
}
